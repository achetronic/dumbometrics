{
  "name": "achetronic/dumbometrics",
  "description": "Set custom metrics on your code. Automagically web-exposed for Prometheus",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "require": {
    "apix/cache": "^1.3",
    "promphp/prometheus_client_php": "^2.6",
    "react/http": "^1.8"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": {
      "*": "dist"
    },
    "platform": {
      "php": ">=7.3"
    }
  },
  "autoload": {
    "psr-4": {
      "Achetronic\\Dumbometrics\\": "src/"
    }
  }
}
